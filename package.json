{
  "name": "frame",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"cypress/e2e/**/*.js\" \"cypress/support/**/*.js\" \"cypress/pages/**/*.js\"",
    "lint:fix": "eslint \"cypress/e2e/**/*.js\" \"cypress/support/**/*.js\" \"cypress/pages/**/*.js\" --fix",
    "prettier-write-format": "prettier --write \"**/*.{js,json,css,html,md}\"",
    "prettier-check-format": "prettier --check \"**/*.{js,json,css,html,md}\"",
    "cypress:qa:open:runner": "npx cypress open --env environment=qa",
    "cypress:dev:open:runner": "npx cypress open --env environment=dev",
    "cypress:qa:run": "npx cypress run --env environment=qa --reporter mochawesome",
    "cypress:dev:run": "npx cypress run --env environment=dev --reporter mochawesome",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --reporter mochawesome",
    "merge-reports": "npx mochawesome-merge cypress/reports/mochawesome/*.json -o cypress/reports/mochawesome.json",
    "generate-report": "npx marge cypress/reports/mochawesome.json --reportDir cypress/reports --inline",
    "test": "npm-run-all cypress:run merge-reports generate-report",
    "clean-reports": "rm -rf 'cypress/reports"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "cypress": "^14.2.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "eslint-plugin-cypress": "^5.2.0"
  }
}
